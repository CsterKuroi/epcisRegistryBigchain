<!-- assets explorer -->
<div class="tab-pane fade" id="tab_assets">

  <div class="row">
    <div class="col-sm-9">

      <div class="panel panel-success">
        <div class="panel-heading">
          <h2 class="panel-title">Assets</h2>
        </div>

        <div class="panel-body">

            <table id="table-channels" class="table table-striped table-bordered col-sm-6">
              <thead>
                <tr>
                  <th>id</th>
                  <th>channel</th>
                  <th>nonce</th>
                </tr>
              </thead>
              <tbody>
                \{{#each assets}}
                <tr>
                  <td>\{{id}}</td>
                  <td>\{{channel}}</td>
                  <td>\{{nonce}}</td>
                </tr>
                \{{/each}}
              </tbody>
            </table>


          </div>
        </div>
    </div>

    <div class="col-sm-3">

      <div class="panel panel-success">

        <div class="panel-body">

        </div>
      </div>

    </div>

  </div>

</div>
