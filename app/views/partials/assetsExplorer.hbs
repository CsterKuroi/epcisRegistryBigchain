<!-- assets explorer -->
<div class="tab-pane fade" id="tab_assets">

  <div class="row">
    <div class="col-sm-11">

      <div class="panel panel-success">
        <div class="panel-heading">
          <h2 class="panel-title">Assets</h2>
        </div>

        <div class="panel-body">

            <table id="table-assets" class="table table-striped table-bordered col-sm-6">
              <thead>
                <tr>
                  <th>bigchain id</th>
                  <th>channel</th>
                  <th>type</th>
                  <th>id</th>
                </tr>
              </thead>
              <tbody id="tbody-assets">

              </tbody>
            </table>


          </div>
        </div>
    </div>

  </div>

</div>

<script id="assetsList-template" type="text/x-handlebars-template">
    \{{#each assets}}
    <tr>
      <td>\{{id}}</td>
      <td>\{{data.channel}}</td>
      <td>\{{data.assetType}}</td>
      <td>\{{data.assetId}}</td>
    </tr>
    \{{/each}}
</script>



